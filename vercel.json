{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/shorturl",
        "methods": ["POST"],
        "dest": "/api/index.js"
      },
      {
        "src": "/api/shorturl/(.*)",
        "dest": "/api/index.js"
      }
    ]
  }
  